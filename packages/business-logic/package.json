{
	"name": "@totallator/business-logic",
	"version": "0.0.1",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./client": {
			"types": "./dist/client.d.ts",
			"default": "./dist/client.js"
		}
	},
	"scripts": {
		"build": "tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json",
		"dev": "tsc --watch --project tsconfig.build.json & tsc-alias --watch -p tsconfig.build.json",
		"clean": "rm -rf dist",
		"test": "vitest",
		"check": "tsc --project tsconfig.json",
		"circular": "madge --circular --extensions ts src/",
		"circular:graph": "madge --circular --extensions ts --image circular-deps.svg src/",
		"format": "prettier --write .",
		"lint": "prettier --check .",
		"knip": "knip"
	},
	"dependencies": {
		"@ai-sdk/anthropic": "^1.2.12",
		"@ai-sdk/google": "^1.2.22",
		"@ai-sdk/groq": "^1.2.9",
		"@ai-sdk/openai": "^1.3.23",
		"@ai-sdk/xai": "^1.2.18",
		"@aws-sdk/client-s3": "^3.777.0",
		"@flystorage/aws-s3": "^1.0.1",
		"@flystorage/file-storage": "^1.0.1",
		"@flystorage/local-fs": "^1.0.1",
		"@node-rs/argon2": "^2.0.2",
		"@openrouter/ai-sdk-provider": "^0.7.3",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"@sveltejs/kit": "^2.26.1",
		"@totallator/context": "workspace:*",
		"@totallator/database": "workspace:*",
		"@totallator/shared": "workspace:*",
		"@types/papaparse": "^5.3.15",
		"ai": "^4.3.19",
		"akahu": "^2.0.0",
		"cronstrue": "^2.57.0",
		"devalue": "^5.1.1",
		"drizzle-orm": "^0.44.3",
		"handlebars": "^4.7.8",
		"lodash-es": "^4.17.21",
		"mathjs": "^14.4.0",
		"nanoid": "^5.1.5",
		"node-schedule": "^2.1.1",
		"papaparse": "^5.5.2",
		"postgres": "^3.4.7",
		"prettier": "^3.6.2",
		"sharp": "^0.33.5",
		"sql-formatter": "^15.6.6",
		"superjson": "^2.2.2",
		"undici-types": "^7.13.0",
		"zod": "^4.0.10"
	},
	"devDependencies": {
		"@trivago/prettier-plugin-sort-imports": "^5.2.2",
		"@types/lodash-es": "^4.17.12",
		"@types/node": "^22.13.14",
		"@types/node-schedule": "^2.1.7",
		"@typescript/analyze-trace": "^0.10.1",
		"knip": "^5.62.0",
		"madge": "^8.0.0",
		"tsc-alias": "^1.8.16",
		"typescript": "^5.9.2",
		"vitest": "^3.1.1"
	}
}
