{
	"name": "totallator-v3",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test:integration": "playwright test",
		"test:unit": "TEST_ENV=true vitest",
		"db:generate": "pnpm drizzle-kit generate:pg --config=\"drizzlePostgres.config.ts\"",
		"db:studio": "pnpm drizzle-kit studio --config=\"drizzlePostgres.config.ts\"",
		"db:undo": "pnpm drizzle-kit drop --config=\"drizzlePostgres.config.ts\"",
		"generate-pwa-assets": "pwa-assets-generator"
	},
	"devDependencies": {
		"@iconify/json": "^2.2.161",
		"@playwright/test": "^1.40.1",
		"@sveltejs/adapter-auto": "^3.0.1",
		"@sveltejs/kit": "^2.0.6",
		"@sveltejs/vite-plugin-svelte": "^3.0.1",
		"@types/lodash-es": "^4.17.12",
		"@types/node-schedule": "^2.1.5",
		"@types/papaparse": "^5.3.14",
		"@typescript-eslint/eslint-plugin": "^6.15.0",
		"@typescript-eslint/parser": "^6.15.0",
		"@vite-pwa/assets-generator": "^0.2.1",
		"@vite-pwa/sveltekit": "^0.3.0",
		"autoprefixer": "^10.4.16",
		"dotenv": "^16.3.1",
		"drizzle-kit": "^0.20.7",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"flowbite": "^2.2.1",
		"flowbite-svelte": "^0.44.21",
		"papaparse": "^5.4.1",
		"postcss": "^8.4.32",
		"postcss-load-config": "^5.0.2",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"superjson": "^2.2.1",
		"svelte": "^4.2.8",
		"svelte-check": "^3.6.2",
		"sveltekit-superforms": "^1.13.1",
		"tailwindcss": "^3.4.0",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"unplugin-icons": "^0.18.1",
		"vite": "^5.0.10",
		"vite-plugin-pwa": "^0.17.4",
		"vitest": "^1.1.0",
		"workbox-window": "^7.0.0"
	},
	"type": "module",
	"dependencies": {
		"@lucia-auth/adapter-postgresql": "^2.0.2",
		"@melt-ui/svelte": "^0.66.2",
		"@sveltejs/adapter-node": "^2.0.2",
		"@tailwindcss/container-queries": "^0.1.1",
		"drizzle-orm": "^0.29.1",
		"handlebars": "^4.7.8",
		"lodash-es": "^4.17.21",
		"lucia": "^2.7.6",
		"nanoid": "^5.0.4",
		"node-schedule": "^2.1.1",
		"postgres": "^3.4.3",
		"skguard": "^0.0.15",
		"skroutes": "^0.0.15",
		"zod": "^3.22.4"
	}
}
