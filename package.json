		"@tailwindcss/container-queries": "^0.1.1",
		"akahu": "^1.15.3",
		"cronstrue": "^2.50.0",
		"devalue": "^5.0.0",
		"drizzle-orm": "^0.33.0",
		"handlebars": "^4.7.8",
		"lodash-es": "^4.17.21",
		"lucia": "^3.2.0",
		"mathjs": "^13.0.3",
		"nanoid": "^5.0.7",
		"node-schedule": "^2.1.1",
		"oslo": "^1.2.1",
