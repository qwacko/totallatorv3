{
	"name": "totallator-v3",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test:integration": "playwright test",
		"test:unit": "TEST_ENV=true vitest",
		"db:generate": "pnpm drizzle-kit generate --config=\"drizzlePostgres.config.ts\"",
		"db:custom": "pnpm drizzle-kit generate --custom --config=\"drizzlePostgres.config.ts\"",
		"db:studio": "pnpm drizzle-kit studio --config=\"drizzlePostgres.config.ts\"",
		"db:undo": "pnpm drizzle-kit drop --config=\"drizzlePostgres.config.ts\"",
		"generate-pwa-assets": "pwa-assets-generator",
		"docs:dev": "vitepress dev docs",
		"docs:build": "vitepress build docs",
		"docs:preview": "vitepress preview docs"
	},
	"devDependencies": {
		"@iconify/json": "^2.2.235",
		"@playwright/test": "^1.46.0",
		"@sveltejs/adapter-auto": "^3.2.2",
		"@sveltejs/kit": "^2.5.20",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@types/lodash-es": "^4.17.12",
		"@types/node-schedule": "^2.1.7",
		"@types/papaparse": "^5.3.14",
		"@typescript-eslint/eslint-plugin": "^8.0.1",
		"@typescript-eslint/parser": "^8.0.1",
		"@vite-pwa/assets-generator": "^0.2.4",
		"@vite-pwa/sveltekit": "^0.6.0",
		"autoprefixer": "^10.4.20",
		"dotenv": "^16.4.5",
		"drizzle-kit": "^0.24.0",
		"echarts": "^5.5.1",
		"eslint": "^9.8.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-drizzle": "^0.2.3",
		"eslint-plugin-svelte": "^2.43.0",
		"flowbite": "^2.5.1",
		"flowbite-svelte": "^0.46.15",
		"mermaid": "^10.9.1",
		"papaparse": "^5.4.1",
		"postcss": "^8.4.41",
		"postcss-load-config": "^6.0.1",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"sql-formatter": "^15.3.2",
		"superjson": "^2.2.1",
		"svelte": "5.0.0-next.215",
		"svelte-check": "^3.8.5",
		"svelte-echarts": "1.0.0-rc3",
		"sveltekit-superforms": "^2.16.1",
		"tailwindcss": "^3.4.9",
		"tslib": "^2.6.3",
		"typescript": "^5.5.4",
		"unplugin-icons": "^0.19.2",
		"vite": "^5.4.0",
		"vite-plugin-pwa": "^0.20.1",
		"vitepress": "^1.3.2",
		"vitepress-plugin-mermaid": "^2.0.16",
		"vitest": "^2.0.5",
		"workbox-window": "^7.1.0"
