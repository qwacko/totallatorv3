{
	"name": "totallator-v3",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test:integration": "playwright test",
		"test:unit": "TEST_ENV=true vitest",
		"db:generate": "pnpm drizzle-kit generate:pg --config=\"drizzlePostgres.config.ts\"",
		"db:custom": "pnpm drizzle-kit generate:pg --custom --config=\"drizzlePostgres.config.ts\"",
		"db:studio": "pnpm drizzle-kit studio --config=\"drizzlePostgres.config.ts\"",
		"db:undo": "pnpm drizzle-kit drop --config=\"drizzlePostgres.config.ts\"",
		"generate-pwa-assets": "pwa-assets-generator",
		"docs:dev": "vitepress dev docs",
		"docs:build": "vitepress build docs",
		"docs:preview": "vitepress preview docs"
	},
	"devDependencies": {
		"@iconify/json": "^2.2.322",
		"@playwright/test": "^1.51.1",
		"@sveltejs/adapter-auto": "^5.0.0",
		"@sveltejs/kit": "^2.20.2",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@types/lodash-es": "^4.17.12",
		"@types/node-schedule": "^2.1.7",
		"@types/papaparse": "^5.3.15",
		"@typescript-eslint/eslint-plugin": "^8.29.0",
		"@typescript-eslint/parser": "^8.29.0",
		"@vite-pwa/assets-generator": "^1.0.0",
		"@vite-pwa/sveltekit": "^1.0.0",
		"autoprefixer": "^10.4.21",
		"dotenv": "^16.4.7",
		"drizzle-kit": "^0.30.6",
		"echarts": "^5.6.0",
		"eslint": "^9.23.0",
		"eslint-config-prettier": "^10.1.1",
		"eslint-plugin-drizzle": "^0.2.3",
		"eslint-plugin-svelte": "^3.4.1",
		"flowbite": "^3.1.2",
		"flowbite-svelte": "^0.48.4",
		"mermaid": "^11.6.0",
		"papaparse": "^5.5.2",
		"postcss": "^8.5.3",
		"postcss-load-config": "^6.0.1",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"sql-formatter": "^15.5.2",
		"superjson": "^2.2.2",
		"svelte": "5.25.3",
		"svelte-check": "^4.1.5",
		"svelte-echarts": "1.0.0-rc3",
		"sveltekit-superforms": "^2.24.0",
		"tailwindcss": "^4.0.17",
		"tslib": "^2.8.1",
		"typescript": "^5.8.2",
		"unplugin-icons": "^22.1.0",
		"vite": "^6.2.4",
		"vite-plugin-pwa": "^1.0.0",
		"vitepress": "^1.6.3",
		"vitepress-plugin-mermaid": "^2.0.17",
		"vitest": "^3.1.1",
		"workbox-window": "^7.3.0"
