{
	"name": "totallator-v3",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test:integration": "playwright test",
		"test:unit": "TEST_ENV=true vitest",
		"db:generate": "pnpm drizzle-kit generate:pg --config=\"drizzlePostgres.config.ts\"",
		"db:custom": "pnpm drizzle-kit generate:pg --custom --config=\"drizzlePostgres.config.ts\"",
		"db:studio": "pnpm drizzle-kit studio --config=\"drizzlePostgres.config.ts\"",
		"db:undo": "pnpm drizzle-kit drop --config=\"drizzlePostgres.config.ts\"",
		"generate-pwa-assets": "pwa-assets-generator",
		"docs:dev": "vitepress dev docs",
		"docs:build": "vitepress build docs",
		"docs:preview": "vitepress preview docs"
	},
	"devDependencies": {
		"@iconify/json": "^2.2.204",
		"@playwright/test": "^1.43.1",
		"@sveltejs/adapter-auto": "^3.2.0",
		"@sveltejs/kit": "^2.5.7",
		"@sveltejs/vite-plugin-svelte": "^3.1.0",
		"@types/lodash-es": "^4.17.12",
		"@types/node-schedule": "^2.1.7",
		"@types/papaparse": "^5.3.14",
		"@typescript-eslint/eslint-plugin": "^7.7.1",
		"@typescript-eslint/parser": "^7.7.1",
		"@vite-pwa/assets-generator": "^0.2.4",
		"@vite-pwa/sveltekit": "^0.4.0",
		"autoprefixer": "^10.4.19",
		"dotenv": "^16.4.5",
		"drizzle-kit": "^0.20.17",
		"echarts": "^5.5.0",
		"eslint": "^9.1.1",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-drizzle": "^0.2.3",
		"eslint-plugin-svelte": "^2.38.0",
		"flowbite": "^2.3.0",
		"flowbite-svelte": "^0.46.1",
		"mermaid": "^10.9.0",
		"papaparse": "^5.4.1",
		"postcss": "^8.4.38",
		"postcss-load-config": "^5.0.3",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.3",
		"prettier-plugin-tailwindcss": "^0.5.14",
		"sql-formatter": "^15.3.1",
		"superjson": "^2.2.1",
		"svelte": "^4.2.15",
		"svelte-check": "^3.7.0",
		"svelte-echarts": "^0.1.1",
		"sveltekit-superforms": "^2.12.6",
		"tailwindcss": "^3.4.3",
		"tslib": "^2.6.2",
		"typescript": "^5.4.5",
		"unplugin-icons": "^0.18.5",
		"vite": "^5.2.10",
		"vite-plugin-pwa": "^0.19.8",
		"vitepress": "^1.1.4",
		"vitepress-plugin-mermaid": "^2.0.16",
		"vitest": "^1.5.2",
		"workbox-window": "^7.1.0"
	},
	"type": "module",
	"dependencies": {
		"@aws-sdk/client-s3": "^3.564.0",
