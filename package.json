		"@sveltejs/adapter-node": "^5.2.12",
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/vite": "^4.1.1",
		"akahu": "^2.0.0",
		"better-auth": "^1.2.5",
		"cronstrue": "^2.57.0",
		"devalue": "^5.1.1",
		"drizzle-orm": "^0.41.0",
		"handlebars": "^4.7.8",
		"lodash-es": "^4.17.21",
		"lucia": "^3.2.2",
		"mathjs": "^14.4.0",
		"nanoid": "^5.1.5",
