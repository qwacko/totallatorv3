{
  "name": "totallator-v3",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "test:integration": "playwright test",
    "test:unit": "TEST_ENV=true vitest",
    "db:generate": "pnpm drizzle-kit generate:pg --config=\"drizzlePostgres.config.ts\"",
    "db:custom": "pnpm drizzle-kit generate:pg --custom --config=\"drizzlePostgres.config.ts\"",
    "db:studio": "pnpm drizzle-kit studio --config=\"drizzlePostgres.config.ts\"",
    "db:undo": "pnpm drizzle-kit drop --config=\"drizzlePostgres.config.ts\"",
    "generate-pwa-assets": "pwa-assets-generator",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.195",
    "@playwright/test": "^1.42.1",
    "@sveltejs/adapter-auto": "^3.1.1",
    "@sveltejs/kit": "^2.5.4",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node-schedule": "^2.1.6",
    "@types/papaparse": "^5.3.14",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "@vite-pwa/assets-generator": "^0.2.4",
    "@vite-pwa/sveltekit": "^0.4.0",
    "autoprefixer": "^10.4.19",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.20.14",
    "echarts": "^5.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-drizzle": "^0.2.3",
    "eslint-plugin-svelte": "^2.35.1",
    "flowbite": "^2.3.0",
    "flowbite-svelte": "^0.44.24",
    "mermaid": "^10.9.0",
    "papaparse": "^5.4.1",
    "postcss": "^8.4.38",
    "postcss-load-config": "^5.0.3",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.2",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "sql-formatter": "^15.3.0",
    "superjson": "^2.2.1",
    "svelte": "^4.2.12",
    "svelte-check": "^3.6.8",
    "svelte-echarts": "^0.1.1",
    "sveltekit-superforms": "^2.11.0",
    "tailwindcss": "^3.4.1",
    "tslib": "^2.6.2",
    "typescript": "^5.4.3",
    "unplugin-icons": "^0.18.5",
    "vite": "^5.2.6",
    "vite-plugin-pwa": "^0.19.7",
    "vitepress": "^1.0.2",
    "vitepress-plugin-mermaid": "^2.0.16",
    "vitest": "^1.4.0",
    "workbox-window": "^7.0.0"
  },
  "type": "module",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.540.0",
    "@flystorage/aws-s3": "^0.1.1",
    "@flystorage/file-storage": "^0.1.1",
    "@flystorage/local-fs": "^0.1.2",
    "@lucia-auth/adapter-drizzle": "^1.0.7",
    "@melt-ui/svelte": "^0.76.2",
    "@sveltejs/adapter-node": "^5.0.1",
    "@tailwindcss/container-queries": "^0.1.1",
    "akahu": "^1.15.3",
    "devalue": "^4.3.2",
    "drizzle-orm": "^0.30.4",
    "handlebars": "^4.7.8",
    "lodash-es": "^4.17.21",
    "lucia": "^3.1.1",
    "mathjs": "^12.4.1",
    "nanoid": "^5.0.6",
    "node-schedule": "^2.1.1",
    "oslo": "^1.1.3",
    "postgres": "^3.4.4",
    "sharp": "^0.33.3",
    "skguard": "^0.0.15",
    "skroutes": "^0.0.15",
    "zod": "^3.22.4"
  }
}