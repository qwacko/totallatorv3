{
	"name": "totallator-v3",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test:integration": "playwright test",
		"test:unit": "TEST_ENV=true vitest",
		"db:generate": "pnpm drizzle-kit generate:pg --config=\"drizzlePostgres.config.ts\"",
		"db:custom": "pnpm drizzle-kit generate:pg --custom --config=\"drizzlePostgres.config.ts\"",
		"db:studio": "pnpm drizzle-kit studio --config=\"drizzlePostgres.config.ts\"",
		"db:undo": "pnpm drizzle-kit drop --config=\"drizzlePostgres.config.ts\"",
		"generate-pwa-assets": "pwa-assets-generator"
	},
	"devDependencies": {
		"@iconify/json": "^2.2.191",
		"@playwright/test": "^1.42.1",
		"@sveltejs/adapter-auto": "^3.1.1",
		"@sveltejs/kit": "^2.5.3",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@types/lodash-es": "^4.17.12",
		"@types/node-schedule": "^2.1.6",
		"@types/papaparse": "^5.3.14",
		"@typescript-eslint/eslint-plugin": "^7.2.0",
		"@typescript-eslint/parser": "^7.2.0",
		"@vite-pwa/assets-generator": "^0.2.4",
		"@vite-pwa/sveltekit": "^0.4.0",
		"autoprefixer": "^10.4.18",
		"dotenv": "^16.4.5",
		"drizzle-kit": "^0.20.14",
		"echarts": "^5.5.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-drizzle": "^0.2.3",
		"eslint-plugin-svelte": "^2.35.1",
		"flowbite": "^2.3.0",
		"flowbite-svelte": "^0.44.24",
		"papaparse": "^5.4.1",
		"postcss": "^8.4.35",
		"postcss-load-config": "^5.0.3",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.2",
		"prettier-plugin-tailwindcss": "^0.5.12",
		"sql-formatter": "^15.2.0",
		"superjson": "^2.2.1",
		"svelte": "^4.2.12",
		"svelte-check": "^3.6.7",
		"svelte-echarts": "^0.1.1",
		"sveltekit-superforms": "^2.9.0",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.6.2",
		"typescript": "^5.4.2",
		"unplugin-icons": "^0.18.5",
		"vite": "^5.1.6",
		"vite-plugin-pwa": "^0.19.2",
		"vitest": "^1.3.1",
		"workbox-window": "^7.0.0"
	},
	"type": "module",
	"dependencies": {
		"@lucia-auth/adapter-drizzle": "^1.0.3",
		"@melt-ui/svelte": "^0.76.0",
		"@sveltejs/adapter-node": "^5.0.1",
		"@tailwindcss/container-queries": "^0.1.1",
		"devalue": "^4.3.2",
		"drizzle-orm": "^0.30.1",
		"handlebars": "^4.7.8",
		"lodash-es": "^4.17.21",
		"lucia": "^3.1.1",
		"mathjs": "^12.4.0",
		"nanoid": "^5.0.6",
		"node-schedule": "^2.1.1",
		"oslo": "^1.1.3",
		"postgres": "^3.4.3",
		"skguard": "^0.0.15",
		"skroutes": "^0.0.15",
		"zod": "^3.22.4"
	}
}
