		"@sveltejs/adapter-node": "^5.0.1",
		"@tailwindcss/container-queries": "^0.1.1",
		"akahu": "^1.15.3",
		"cronstrue": "^2.50.0",
		"devalue": "^5.0.0",
		"drizzle-orm": "^0.30.9",
		"handlebars": "^4.7.8",
		"lodash-es": "^4.17.21",
		"lucia": "^3.2.0",
		"mathjs": "^12.4.2",
		"nanoid": "^5.0.7",
		"node-schedule": "^2.1.1",
		"oslo": "^1.2.0",
		"postgres": "^3.4.4",
		"sharp": "^0.33.3",
